{
  "name": "coder-blog",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "NODE_ENV=production npm run prisma:generate && node -r dotenv/config server.js",
    "prisma:generate": "prisma generate",
    "postinstall": "npx prisma generate",
    "dev:backend": "NODE_ENV=development node -r dotenv/config server.js",
    "dev:frontend": "vite",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "build": "NODE_ENV=production npx prisma generate && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "prisma:push": "npx prisma db push"
  },
  "dependencies": {
    "@prisma/client": "^6.9.0",
    "axios": "^1.8.4",
    "bcrypt": "^6.0.0",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.15.0",
    "multer": "^1.4.5-lts.2",
    "nodemailer": "^7.0.3",
    "path-to-regexp": "^8.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.4.1"
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/node": "^22.15.3",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "eslint": "^9.9.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "nodemon": "^3.1.10",
    "postcss": "^8.4.47",
    "prisma": "^6.9.0",
    "tailwindcss": "^3.4.12",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vite": "^6.3.2",
    "vite-plugin-svgr": "^4.3.0"
  }
}
